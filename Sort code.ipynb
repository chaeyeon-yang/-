    "# 거품 정렬 \n",
    "def bubbleSort(list1):\n",
    "    for i in range(len(list1)):\n",
    "        for j in range(len(list1)):\n",
    "            if list1[i] < list1[j]:\n",
    "                list1[i], list1[j] = list1[j], list1[i]\n",
    "    return list1\n",
    "\n",
    "\n",
    "# 테스트 코드\n",
    "list1 = [20, 23, 15, 7, 98, 56, 1]\n",
    "bubbleSort(list1)"
   ]
