   "source": [
    "# 삽입 정렬 \n",
    "\n",
    "def insertion(list1):\n",
    "    for i in range(1, len(list1)):\n",
    "        j = i\n",
    "        while j>0 and list1[j] < list1[j-1]:\n",
    "            list1[j-1], list1[j] = list1[j], list1[j-1]\n",
    "            j -= 1\n",
    "    return list1\n",
    "\n",
    "\n",
    "\n",
    "list1 = [11, 3, 28, 43, 9, 4]\n",
    "insertion(list1)"
   ]
